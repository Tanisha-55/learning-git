public class Node {
    private String name;
    private Long id;
    private Integer lvl;
    private List<Node> upstream = new ArrayList<>();
    private List<Node> downstream = new ArrayList<>();

    // Constructor, getters, setters
    public Node(String name, Long id, Integer lvl) {
        this.name = name;
        this.id = id;
        this.lvl = lvl;
    }
}
